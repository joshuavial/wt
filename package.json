{
  "name": "@your-org/wt",
  "version": "1.0.0",
  "description": "Git Worktree Manager - A powerful CLI tool for managing Git worktrees with Docker and tmux integration",
  "keywords": [
    "git",
    "worktree",
    "docker",
    "tmux",
    "cli",
    "development",
    "workflow"
  ],
  "homepage": "https://github.com/your-org/wt#readme",
  "bugs": {
    "url": "https://github.com/your-org/wt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/wt.git"
  },
  "license": "MIT",
  "author": "Your Name <your.email@example.com>",
  "main": "dist/index.js",
  "bin": {
    "wt": "dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dev": "tsx src/cli.ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/glob": "^9.0.0",
    "@types/prompts": "^2.4.9",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.3",
    "ini": "^4.1.1",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/ini": "^4.1.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.13.1"
}